{
  "name": "@splitsoftware/splitio",
  "version": "7.0.0-canary.0",
  "description": "Split SDK",
  "files": [
    "*.md",
    "docs",
    "es6",
    "lib",
    "umd"
  ],
  "repository": "splitio/javascript-client",
  "homepage": "https://github.com/splitio/javascript-client#readme",
  "bugs": "https://github.com/splitio/javascript-client/issues",
  "license": "Apache-2.0",
  "author": "Facundo Cabrera <facundo@split.io>",
  "main": "lib/core/node",
  "jsnext:main": "lib/core/node",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "cross-env": "^2.0.1",
    "csv-streamify": "^3.0.4",
    "eslint": "^3.4.0",
    "fetch-mock": "^5.1.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-tap": "^3.0.1",
    "karma-webpack": "^1.8.0",
    "rimraf": "^2.5.4",
    "tape": "^4.6.0",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./src -d lib --ignore '__tests__'",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./src -d es6 --ignore '__tests__'",
    "build-umd": "cross-env NODE_ENV=development webpack src/core umd/split.js",
    "build-min": "cross-env NODE_ENV=production webpack -p src/core umd/split.min.js",
    "lint": "eslint src",
    "test-browser-local": "cross-env NODE_ENV=test karma start karma/local.karma.conf.js",
    "test-browser-e2e-local": "cross-env NODE_ENV=test karma start karma/e2e.local.karma.conf.js",
    "test-browser-sauce": "cross-env NODE_ENV=test karma start karma/sauce.karma.conf.js",
    "test-browser-e2e-sauce": "cross-env NODE_ENV=test karma start karma/e2e.sauce.karma.conf.js",
    "test-browser-ci": "cross-env NODE_ENV=test karma start karma/ci.karma.conf.js",
    "test-browser-e2e-ci": "cross-env NODE_ENV=test karma start karma/e2e.ci.karma.conf.js",
    "test-node": "tape -r babel-register \"src/*/__tests__/**/!(browser).spec.js\""
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "debug": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "warning": "^3.0.0"
  }
}
